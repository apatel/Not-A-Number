  <tr>
    <td class="p5"><b><%= link_to target_list.title, polymorphic_url(target_list) %></b></td>
    <td ><b><%= target_list.output_text %></b></td>
    <td ><b><%= target_list.description %></b></td>
    <td class="p5"><%= link_to 'Edit', edit_polymorphic_path(target_list, :source => "dialog_index"), :title => "Edit #{target_list.class} ID #{target_list.id}", :onclick => "Modalbox.show(this.href, {title: this.title, width: 600}); return false;" %></td>
    <td class="p5"><%= link_to_function 'Delete', render_destroy(target_list, "dialog_index", "Delete #{target_list.class} ID #{target_list.id}") %></td>
  </tr>
