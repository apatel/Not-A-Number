<h1>Register</h1>

<% form_for @user, :url => createuser_project_url do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>
  <%= f.hidden_field(:project, :value=>@project.id)%>
  <%= f.submit "Register" %>
  <!--<%= oauth_register_button :value => "Register with Twitter" %><br />-->
<% end %>

